{
  "name": "front-template",
  "version": "0.0.14",
  "description": "",
  "main": "out/main.js",
  "author": {
    "name": "xuhuale",
    "email": "xuhuale@star-net.cn"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production && tsc && electron out/main.js",
    "electron:dev": "cross-env NODE_ENV=development && tsc &&  electron out/main.js",
    "electron:watch": "nodemon --config ./native/config/nodemon.json",
    "electron:build": "cross-env NODE_ENV=production && tsc ",
    "fronted:dev": "cd frontend && npm run dev",
    "fronted:build": "cd frontend && npm run build",
    "fronted:copy": "mkdir -p out/public/ && cp -r frontend/dist/ out/public/dist/",
    "lint": "eslint native/**/ --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "build": "npm run fronted:build && npm run  fronted:copy && npm run electron:build",
    "publish-build-pre": "npm run fronted:build && npm run  fronted:copy",
    "ev-publish-build": "node ./script/publish.js",
    "build-w": "electron-builder -w=nsis --x64",
    "build-w-32": "electron-builder -w=nsis --ia32",
    "build-w-64": "electron-builder -w=nsis --x64",
    "build-w-arm64": "electron-builder -w=nsis --arm64",
    "build-wz": "electron-builder -w=7z --x64",
    "build-wz-32": "electron-builder -w=7z --ia32",
    "build-wz-64": "electron-builder -w=7z --x64",
    "build-wz-arm64": "electron-builder -w=7z --arm64",
    "build-m": "electron-builder -m --x64 --arm64",
    "build-m-arm64": "electron-builder -m --arm64",
    "build-l": "electron-builder -l=deb --x64",
    "build-l-32": "electron-builder -l=deb --ia32",
    "build-l-64": "electron-builder -l=deb --x64",
    "build-l-arm64": "electron-builder -l=deb --arm64",
    "build-l-armv7l": "electron-builder -l=deb --armv7l",
    "build-lr-64": "electron-builder -l=rpm --x64",
    "build-lp-64": "electron-builder -l=pacman --x64",
    "rebuild": "electron-rebuild",
    "cz": "cz"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@electron/rebuild": "^3.2.9",
    "@types/electron": "^1.6.10",
    "@types/koa": "^2.13.6",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.3",
    "shelljs": "^0.8.4",
    "chalk": "^4.1.0",
    "clear-console": "^1.1.0",
    "spinnies": "^0.5.1",
    "inquirer": "^7.3.3"
  },
  "dependencies": {
    "get-port": "^5.1.1",
    "koa": "^2.14.1",
    "koa-static": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
